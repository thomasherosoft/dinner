<p id="notice"><%= notice %></p>
<div class="container">
<div class="table-responsive">
  <table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>News</th>
      <th>Michelin*</th>
      <th>Reviews</th>
      <th>Address</th>
      <th>City</th>
      <th>Website</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.name %></td>
        <td><%= post.inthenews %></td>
        <td><%= post.michelin_status %></td>
        <td><%= post.google_reviews %></td>
        <td><%= post.address %></td>
        <td><%= post.city %></td>
        <td><a href="<%= post.website %>">Click</a></td>
        <td><%= post.phone %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
    </div>
  </div>



<br>
