<p id="notice"><%= notice %></p>
<div class="container">
<h2>Michelin Star Restaurants in the UK</h2>
<div class="table-responsive">
  <table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>In the news</th>
      <th>Michelin status</th>
      <th>Google reviews</th>
      <th>Address</th>
      <th>City</th>
      <th>Website</th>
      <th>Phone</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.name %></td>
        <td><%= post.inthenews %></td>
        <td><%= post.michelin_status %></td>
        <td><%= post.google_reviews %></td>
        <td><%= post.address %></td>
        <td><%= post.city %></td>
        <td><a href="<%= post.website %>">Click</a></td>
        <td><%= post.phone %></td>
        <td><%= post.longitude %></td>
        <td><%= post.latitude %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
    </div>
  </div>



<br>

<%= link_to 'New Post', new_post_path %>
